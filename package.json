{
  "name": "todo-list-electron",
  "productName": "DoThat",
  "version": "1.0.5",
  "description": "A beautiful app to plan and organize your exams and tasks",
  "main": ".webpack/x64/main/index.js",
  "homepage": "https://thatqne.github.io/dothat",
  "scripts": {
    "start": "rimraf .webpack && cross-env NODE_ENV=development ELECTRON_ENABLE_LOGGING=1 FORCE_COLOR=1 FORCE_HMR=true electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "publish": "cross-env NODE_ENV=production electron-forge publish",
    "clean": "rimraf out .webpack",
    "lint": "echo \"No linting configured\"",
    "publish-windows": "powershell -ExecutionPolicy ByPass -File ./publish-with-token.ps1 -UseMakeCmd \"npm run make\""
  },
  "keywords": [
    "electron",
    "exam",
    "planner",
    "todo",
    "tasks",
    "organization"
  ],
  "author": {
    "name": "ThatQne",
    "email": "82828236+ThatQne@users.noreply.github.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.1",
    "@babel/preset-react": "^7.27.1",
    "@electron-forge/cli": "^7.8.0",
    "@electron-forge/maker-deb": "^7.8.0",
    "@electron-forge/maker-rpm": "^7.8.0",
    "@electron-forge/maker-squirrel": "^7.8.0",
    "@electron-forge/maker-zip": "^7.8.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.0",
    "@electron-forge/plugin-fuses": "^7.8.0",
    "@electron-forge/plugin-webpack": "^7.8.0",
    "@electron-forge/publisher-github": "^7.8.0",
    "@electron/fuses": "^1.8.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "electron": "36.1.0",
    "electron-builder": "^26.0.12",
    "electron-notarize": "^1.2.2",
    "fs-extra": "^11.3.0",
    "node-loader": "^2.1.0",
    "rimraf": "^6.0.1",
